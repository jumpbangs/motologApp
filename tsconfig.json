{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "types/*": ["./types/*"],
      "store/*": ["./store/*"],
      "theme/*": ["./theme/*"],
      "utils/*": ["./utils/*"],
      "services/*": ["./app/services/*"],
      "components/*": ["./app/components/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
